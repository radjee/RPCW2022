extends layout

block content
    .w3-card-4.w3-margin
        header(class="w3-container w3-teal")
            h3 Successful student insertion
        .w3-container
            p #{rec.nome} successfully stored with id: #{rec._id}.
        .w3-container.w3-border.w3-margin
            pre= rec
        button(class='w3-button w3-block w3-border w3-border-teal' style='width:50%')
            a(href='/students') Back to Student's List

        footer(class="w3-container w3-teal")
            address Generated by StudentApp :: 2020-Nov, jcr
